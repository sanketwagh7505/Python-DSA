   ],
   "source": [
    "flag = 0\n",
    "i = 1\n",
    "\n",
    "odd_list = []\n",
    "\n",
    "while True:\n",
    "    if i%2 !=0:\n",
    "        odd_list.append(i)\n",
    "        flag = flag + 1\n",
    "\n",
    "    if flag == 25:\n",
    "        break\n",
    "    i = i+1\n",
    "\n",
    "print(odd_list)        "
   ]
