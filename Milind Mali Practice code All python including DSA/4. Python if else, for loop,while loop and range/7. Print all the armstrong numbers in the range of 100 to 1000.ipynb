   ],
   "source": [
    "armstrong_list =[]\n",
    "\n",
    "for i in range(100, 1000):\n",
    "    a = i % 10\n",
    "    num = i // 10\n",
    "    b = num %10\n",
    "    c = num // 10\n",
    "\n",
    "    if (a**3) + (b**3) + (c**3)==i:\n",
    "        armstrong_list.append(i)\n",
    "\n",
    "print(armstrong_list)"
   ]
  }
